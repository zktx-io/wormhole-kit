{
  "include": ["src"],
  "compilerOptions": {
    "outDir": "lib",
    "target": "es5",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  }
}
